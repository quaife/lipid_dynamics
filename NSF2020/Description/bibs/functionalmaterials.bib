	Journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
	Primaryclass = {cond-mat},
	Title = {{Elastic deformation of a fluid membrane upon colloid binding}},
	Year = {2004},
	Bdsk-Url-1 = {https://doi.org/10.1103/PhysRevE.69.031903}}

@article{Agudo-Canalejo2015,
	Abstract = {Biological and biomimetic membranes display complex shapes with nonuniform curvature. Because the interaction of adhesive nanoparticles with such membranes depends on the local membrane curvature, different segments of the same membrane can differ in their engulfment behavior. For a single vesicle in contact with many nanoparticles, we predict ten distinct engulfment patterns as well as morphological transitions between these patterns, which are directly accessible to experiment.},
	Author = {Agudo-Canalejo, Jaime and Lipowsky, Reinhard},
	Date-Added = {2019-11-12 16:12:47 -0500},
