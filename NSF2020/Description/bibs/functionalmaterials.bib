	Issn = {15393755},
	Journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
	Title = {{Budding and vesiculation induced by conical membrane inclusions}},
	Year = {2009},
	Bdsk-Url-1 = {https://doi.org/10.1103/PhysRevE.80.031901}}

@article{Deserno2004,
	Abstract = {When a colloidal particle adheres to a fluid membrane, it induces elastic deformations in the membrane which oppose its own binding. The structural and energetic aspects of this balance are theoretically studied within the framework of a Helfrich Hamiltonian. Based on the full nonlinear shape equations for the membrane profile, a line of continuous binding transitions and a second line of discontinuous envelopment transitions are found, which meet at an unusual triple point. The regime of low tension is studied analytically using a small gradient expansion, while in the limit of large tension scaling arguments are derived which quantify the asymptotic behavior of phase boundary, degree of wrapping, and energy barrier. The maturation of animal viruses by budding is discussed as a biological example of such colloid-membrane interaction events.},
	Archiveprefix = {arXiv},
	Arxivid = {cond-mat/0303656},
