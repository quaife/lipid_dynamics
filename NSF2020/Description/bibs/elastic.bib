	Doi = {10.1073/pnas.1119442109},
	File = {:Users/rryham/Library/Application Support/Mendeley Desktop/Downloaded/Aeffner et al. - 2012 - Energetics of stalk intermediates in membrane fusion are controlled by lipid composition.pdf:pdf},
	Isbn = {1119442109},
	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences},
	Keywords = {Energy Metabolism,Lipids,Lipids: chemistry,Membrane Fusion,Water,Water: chemistry,X-Ray Diffraction},
	Number = {25},
	Pages = {E1609--E1618},
	Pmid = {22589300},
	Title = {{Energetics of stalk intermediates in membrane fusion are controlled by lipid composition}},
