@Article{DiCarlo2019,

author="DiCarlo, Antonio",

title="Continuum Mechanics as a Computable Coarse-Grained Picture of Molecular Dynamics",

journal="Journal of Elasticity",

year="2019",

month="Apr",

day="01",

volume="135",

number="1",

pages="183--235",

abstract="In this paper, I determine the minimum amount of information that continuum mechanics needs to obtain from Newtonian molecular dynamics, in order to avail itself of stress-strain responses uniformly valid for a vast range of macroscopic regimes, being quantitatively determined by microscopic physical properties. Described from the opposite, bottom-up point of view, the procedure I put forward uses the basic kinematic and dynamical machinery of continuum mechanics to upscale molecular dynamics to the macroscopic level in a practicable and efficient way.",

issn="1573-2681",

doi="10.1007/s10659-019-09734-y",

url="https://doi.org/10.1007/s10659-019-09734-y"

}